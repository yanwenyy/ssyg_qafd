<!--pages/course/course.wxml-->
<view class="container">
  <view class="course-head">
    <view class="inline-block course-search">
      <image src="../../img/search-img.png" mode="aspectFit"></image>
      <input class="inline-block box-sizing" placeholder="请输入要搜索的内容"></input>
    </view>
    <view class="inline-block">搜索</view>
  </view>
  <view class="course-body">
    <view class="course-list shadow-model box-sizing" bindtap="goContent">
      <image src="" mode="aspectFit"></image>
      <view class="inline-block course-list-right">
        <view class="course-list-right-name">房地产促销中买房赠物 税务处理</view>
        <view>王家集</view>
        <view class="vip-course">VIP课程</view>
      </view>
    </view>
  </view>
</view>
