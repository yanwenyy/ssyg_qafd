/* pages/mine/mineCollection/mineCollection.wxss */
@import "../../industry/industryFXTS/industryFXTS.wxss";
.ifx-list-head{
  margin-bottom: 30rpx;
}
.ifx-list-title{
  margin: 0 0 19rpx 0;
}
.mc-date{
  font-size: 24rpx;
  color:#999;
}
.ifx-list-head>image{
  width: 34rpx;
  height: 34rpx;
  vertical-align: middle;
  margin-right:9rpx;
}
.ifx-list-head-name{
  font-size: 20rpx;
  color:#999;
}
.ifx-list-label{
  line-height: 41rpx;
  margin-top:-5rpx;
  margin-bottom: 10rpx;
}
.ifx-list-right{
  width: 310rpx;
  height: 175rpx;
  margin-right: 22rpx;
  vertical-align: top;
  background: #eee;
  border-radius: 10rpx;
}
.ifx-list-left{
  width: 45%;
  word-break: break-all;
}