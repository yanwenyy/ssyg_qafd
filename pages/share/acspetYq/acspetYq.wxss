/* pages/share/acspetYq/acspetYq.wxss */
.container{
  background: #F4F7FD;
}
.ayq-model{
  padding: 42rpx 35rpx;
}
.ayq-show{
  width: 100%;
  height: 535rpx;
  position: relative;
}
.ayq-show>image,.ayq-show-msg{
  width: 100%;
  height:100%;
  position: absolute;
  top:0;
  left: 0;
}
.ayq-show-msg{
  padding:43rpx 33rpx;
}
.ayq-msg-title{
  text-align: center;
  font-size: 38rpx;
  color:#9B601F;
  font-weight: bold;
  margin-bottom: 84rpx;
}
.ayq-msg-headimg{
  width: 99rpx;
  height: 99rpx;
  margin-right: 24rpx;
  vertical-align: top;
}
.ayq-msg-name{
  font-size: 32rpx;
  font-weight: bold;
  line-height: 48rpx;
}
.ayq-msg-yq{
  font-size: 30rpx;
  line-height: 48rpx;
}
.ayq-msg-cicon{
  width: 28rpx;
  height: 32rpx;
  vertical-align: top;
  margin-right: 11rpx;
  background: #eee;
  margin-top: 5rpx;
}
.ayq-msg-cname{
  font-size: 30rpx;
  color:#B07937;
  line-height: 45rpx;
  width: 91%;
  vertical-align: top;
}
.ayq-msg-footer{
  width: 100%;
  padding:38rpx;
  background: #FDFBF9;
  border:1rpx solid #FDFBF9;
  border-radius: 16rpx;
  margin-top: 39rpx;
}
.jionTeamBtn{
  width: 100%!important;
  height: 88rpx;
  line-height: 88rpx;
  margin-top: 69rpx;
  color:#fff;
  font-size: 32rpx;
  background: #28B7A3;
}
.jumpJion{
  width: 80%;
  height: 87rpx;
  line-height: 87rpx;
  border:1rpx solid #3EC1AE;
  color:#3EC1AE;
  font-size: 32rpx;
  border-radius: 44rpx;
  position: fixed;
  bottom: 124rpx;
  left: 10%;
  z-index: 999;
  text-align: center;
  background: #fff;
}
.jumpJion>image{
  width: 34rpx;
  height: 34rpx;
  vertical-align: middle;
  margin-top: -6rpx;
}