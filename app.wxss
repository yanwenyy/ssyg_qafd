/**app.wxss**/
.container {
  width: 100%;
  min-height: 100vh;
  height: auto;
  padding: 32rpx 34rpx;
  box-sizing: border-box;
}

.inline-block {
  display: inline-block;
}

.box-sizing {
  box-sizing: border-box
}

.shadow-model {
  width: 100%;
  height: auto;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 0px 26rpx 0px rgba(3, 0, 0, 0.05);
  border-radius: 12rpx;
}
.green{
  color:#28B7A3;
}