/**app.wxss**/
.container {
  width: 100%;
  min-height: 100vh;
  height: auto;
  padding: 32rpx 34rpx;
  box-sizing: border-box;
}
.head-img{
  border-radius: 50%;
  background: #eee;
}
.body{
  background: #F6F6F6;
}
.inline-block {
  display: inline-block;
}

.box-sizing {
  box-sizing: border-box
}

.shadow-model {
  width: 100%;
  height: auto;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 0px 26rpx 0px rgba(3, 0, 0, 0.05);
  border-radius: 12rpx;
}
.green{
  color:#28B7A3;
}
.red{
  color:#F64048;
}
.mask{
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,0.3);
  position: fixed;
  top:0;
  left:0;
  z-index: 999;
}
.shadow{
  width:100%;
  height:100%;
  background: rgba(0,0,0,0.3);
  position: fixed;
  top:0;
  left:0;
  z-index: 9999
}
/* 行业单选框样式 */
.wx-radio-input {
  width: 34rpx!important;
  height: 34rpx!important;
}
/*  */
button{
  padding:0!important;
}